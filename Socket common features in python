import socket
try:
    print("Fully qualified domain name: " + socket.getfqdn("8.8.8.8"))
    print("Hostname to IP address" + socket.gethostbyname("www.python.org"))
    print("Hostname to IP addedd, extended: " + str(socket.gethostbyname_ex("www.python.org")))
    print("Get hostname of local machine: " + socket.gethostname())
except Exception as err:
    print("Error: " + str(err))

